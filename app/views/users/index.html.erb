<section class="users-section">
  <ul class="users-list">
    <% @users.each do |user| %>
    <li>
        Name: <%= user.name %>
        <span class="profile-link">
          <%= link_to 'See Profile',  user_path(user), class: 'profile-link' %>
        </span>
        <span class="profile-link">
          <%= link_to('Invite', invite_path(user_id: user.id), method: :post) %>
        </span>
      </li>
    <% end %>
  </ul>
</section>
